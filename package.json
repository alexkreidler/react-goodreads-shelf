{
  "name": "react-goodreads-shelf",
  "type": "module",
  "version": "3.0.0",
  "description": "A React widget for displaying a user's public book shelf",
  "main": "./dist/index",
  "typings": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "repository": "https://github.com/kylekarpack/react-goodreads-shelf",
  "author": "Kyle Karpack <kylekarpack+rgg@gmail.com>",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=14"
  },
  "engineStrict": true,
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-controls": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/addon-storysource": "^6.5.10",
    "@storybook/addons": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/node": "^17.0.12",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@vitejs/plugin-react": "^2.1.0",
    "@vitest/coverage-c8": "^0.23.2",
    "c8": "^7.11.0",
    "gts": "^3.1.0",
    "jsdom": "^19.0.0",
    "prettier": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "release-it": "^15.4.2",
    "typescript": "^4.8.3",
    "vite": "^3.1.0",
    "vite-plugin-dts": "^1.5.0",
    "vitest": "^0.23.1"
  },
  "scripts": {
    "test": "vitest run --reporter=verbose",
    "test:watch": "vitest --reporter=verbose",
    "test:related": "vitest related",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prepublish": "npm run build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs-build",
    "lint": "gts lint src/**/*.{ts,tsx}",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix src/**/*.{ts,tsx}",
    "release": "release-it"
  },
  "release-it": {
    "github": {
      "release": true,
      "web": true
    }
  }
}
